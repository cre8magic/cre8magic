{
    "name": "tosic.cre8magic.staticwebassets",
    "version": "00.00.10",
    "description": "cre8magic static web assets",
    "type": "module",
    "main": "(none)",
    "scripts": {
        "clean": "rimraf dist",
        "build:ambient": "vite build",
        "build:interop": "tsc --project tsconfig.interop.json",
        "build": "npm run clean && npm run build:ambient && npm run build:interop",
        "serve": "concurrently \"vite build --watch\" \"tsc --watch --project tsconfig.interop.json\"",
        "pack": "npm run build"
    },
    "author": "2sic internet solutions",
    "license": "MIT",
    "repository": "https://github.com/2sic/cre8magic",
    "devDependencies": {
        "@types/node": "^22.13.2",
        "rimraf": "^5.0.10",
        "typescript": "^5.0.0",
        "vite": "^6.1.0",
        "concurrently": "^9.1.2"
    },
    "dependencies": {
        "bootstrap": "^5.2.1"
    }
}